    "/api/wall/list": {
      "get": {
        "summary": "获取用户发布的表白项列表",
        "description": "获取用户发布的表白项列表",
        "parameters": [
          {
            "name": "pageNum",
            "type": "string",
            "description": "分页页码",
            "default": "0",
            "require": true,
            "in": "query"
          },
          {
            "name": "pageSize",
            "type": "string",
            "description": "每页数量",
            "default": "20",
            "require": true,
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "success",
            "schema": {
              "type": "object",
              "properties": {
                "code": {
                  "type": "string",
                  "enum": [
                    100001,
                    100002,
