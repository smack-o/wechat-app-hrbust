                    "SUCCESS",
                    "ERR_PARAM",
                    "SEN_MESSAGE",
                    "NOT_WECHAT_AUTH",
                    "NOT_LOGIN",
                    "NOT_AUTH",
                    "ERROR",
                    "FAIL_UPLOAD"
                  ]
                },
                "message": {
                  "type": "string"
                },
                "result": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "string",
                        "description": "表白墙id",
                        "default": ""
                      },
                      "isPublisher": {
                        "type": "boolean",
                        "description": "是否是发布者",
                        "default": false
                      },
                      "isLike": {
                        "type": "boolean",
                        "description": "是否点赞",
                        "default": false
                      },
                      "likeCount": {
                        "type": "string",
                        "description": "点赞数",
                        "default": 0
                      },
                      "isCollect": {
                        "type": "boolean",
                        "description": "是否收藏",
                        "default": false
                      },
                      "collectCount": {
                        "type": "number",
                        "description": "收藏数量",
                        "default": 0
                      },
                      "publisherId": {
                        "type": "string",
                        "description": "发布者id",
                        "default": ""
                      },
                      "to": {
                        "type": "string",
                        "description": "wall id",
                        "default": ""
                      },
                      "tel": {
                        "type": "string",
                        "description": "手机号",
                        "default": ""
                      },
                      "content": {
                        "type": "string",
                        "description": "内容",
                        "default": ""
                      },
                      "photos": {
                        "type": "array",
                        "description": "图片列表",
                        "default": [],
                        "items": {
                          "type": "object",
                          "properties": {
                            "key": {
                              "type": "string",
                              "description": "图片 url",
                              "default": ""
                            },
                            "status": {
                              "type": "string",
                              "enum": [
                                "0",
                                "1",
                                "2"
                              ]
                            }
                          },
                          "required": [
                            "key",
                            "status"
                          ]
                        }
                      },
                      "isDelete": {
                        "type": "boolean",
                        "description": "是否删除",
                        "default": false
                      }
                    },
                    "required": [
                      "_id",
                      "isPublisher",
                      "isLike",
                      "likeCount",
                      "isCollect",
                      "collectCount",
                      "publisherId",
                      "to",
                      "tel",
                      "content",
                      "isDelete"
                    ]
